@article{mccahillNonworkAccessibilityRelated2018,
	title = {Non-work accessibility and related outcomes},
	volume = {29},
	issn = {22105395},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2210539517300998},
	doi = {10.1016/j.rtbm.2018.07.002},
	abstract = {Accessibility metrics, which describe the ease of reaching destinations, are widely recognized as valuable indicators of transportation system performance. After decades of academic research on the subject, accessibility metrics are gaining use in practice. The most notable applications, however, focus solely on access to jobs. While commute travel is closely related to peak period travel demand, it makes up only a small share of overall travel. This study presents a measure of local access to a wide range of non-work destinations, calibrated using data from Virginia. It focuses speciﬁcally on walking access as a proxy for multimodal accessibility and supportive land uses. The metric is intuitive, it can be calculated using available software and data, and it relates to important outcomes such as travel behavior and economic productivity. This work presents an opportunity for practitioners to incorporate accessibility metrics in various decision making applications and improve upon them as their knowledge and use of these metrics grow.},
	pages = {26--36},
