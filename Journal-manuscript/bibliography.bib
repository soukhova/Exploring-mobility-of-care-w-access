@article{soukhovMultimodalSpatialAvailability2024,
	title = {Multimodal spatial availability: A singly-constrained measure of accessibility considering multiple modes},
	volume = {19},
	rights = {All rights reserved},
	issn = {1932-6203},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0299077},
	doi = {10.1371/journal.pone.0299077},
	shorttitle = {Multimodal spatial availability},
	abstract = {Place-based accessibility measures communicate the potential interaction with opportunities at a zone that populations can access. Recent research has explored the implications of how opportunities are counted by different accessibility methods. In conventional measures, opportunities are multiply counted if more than one zone offers access to the same opportunity. This multi-count of opportunities leads to values of accessibility that are difficult to interpret. A possible solution to enhance the meaning-making of accessibility results is by constraining the calculations to match a known quantity. This ensures all zonal values sum up to a predetermined quantity (i.e., the total number of opportunities). In this way, each value can be meaningfully related to this total. A recent effort that implements this solution is spatial availability, a singly-constrained accessibility measure. In this paper, we extend spatial availability for use in the case of multiple modes or more generally, heterogeneous population segments with distinct travel behaviors. After deriving a multimodal version of spatial availability, we proceed to illustrate its features using a synthetic example. We then apply it to an empirical example of low emission zones in Madrid, Spain. We conclude with suggestions for future research and its use in evaluating policy interventions.},
	pages = {e0299077},
	number = {2},
	journaltitle = {{PLOS} {ONE}},
	shortjournal = {{PLOS} {ONE}},
	author = {Soukhov, Anastasia and Tarriño-Ortiz, Javier and Soria-Lara, Julio A. and Páez, Antonio},
	urldate = {2024-03-10},
	year = {2024},
	langid = {english},
	note = {Publisher: Public Library of Science},
	keywords = {Open data, Transportation, Employment, Spain, Land use, Urban areas, Jobs, Science policy},
	file = {Soukhov et al_2024_Multimodal spatial availability.pdf:C\:\\Users\\user\\Zotero\\storage\\7XFTBUTF\\Soukhov et al_2024_Multimodal spatial availability.pdf:application/pdf},
