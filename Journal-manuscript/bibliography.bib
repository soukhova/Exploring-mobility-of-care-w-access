@article{graellsCityCitiesMeasuring2021,
   author = {Graells-Garrido, E. and Serra-Burriel, F. and Rowe, F. and Cucchietti, F. M. and Reyes, P.},
   title = {A city of cities: Measuring how 15-minutes urban accessibility shapes human mobility in Barcelona},
   journal = {PLoS One},
